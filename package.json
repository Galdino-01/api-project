    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "mysql2": "^3.6.5",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
